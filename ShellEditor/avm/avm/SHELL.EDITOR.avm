       .OR $1000
       .TF INSTALL.SHELL.EDITOR.OBJ,D1
       .IN INSTALL.SHELL.EDITOR,D1
       .IN EDITOR.STRING.FNS,D1
* -------------------------------
* UNINSTALL EDITOR
* -------------------------------
       .OR $1000
       .TF UNINSTALL.SHELL.EDITOR.OBJ,D1
       .IN UNINSTALL.SHELL.EDITOR,D1
* -------------------------------
* DRIVER
* -------------------------------
       .OR $9CDA
       .TF SHELL.EDITOR.DRIVER.OBJ,D1
DRIVER
       .IN DRIVER,D1
