# -------------------------------------
# TEST FUNCTION RETURN VALUE
# -------------------------------------

CONST MON_CROUT  $FD8E;

DATA STR  "SHOULD BE 0000FFFF: ";

FN PRINT_STRING(STR:2);
FN PRINT_HEX(N:2,SIZE:1);

FN MAIN() {
   VAR A:4 = $FFFF;
   VAR B:4 = RTN(A);
   # PRINT A
   PRINT_STRING(STR);   
   PRINT_HEX(@A,4);
   MON_CROUT();
   # PRINT B
   PRINT_STRING(STR);
   PRINT_HEX(@B,4);
   MON_CROUT();
}

FN:4 RTN(N:4) {
   RETURN N;
}

INCLUDE "IO";
