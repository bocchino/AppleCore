DATA STR  "SHOULD BE 0000FFFF: ";

FN MAIN() {
   VAR A:4 = $FFFF;
   VAR B:4 = RTN(A);
   # PRINT A
   PRINT_STRING(STR);   
   PRINT_HEX(@A,4);
   MON_CROUT();
   # PRINT B
   PRINT_STRING(STR);
   PRINT_HEX(@B,4);
   MON_CROUT();
}

FN:4 RTN(N:4) {
   RETURN N;
}

