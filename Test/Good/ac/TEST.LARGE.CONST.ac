CONST MON_CROUT $FD8E;

DATA STR "SHOULD BE FFFFFFFFFFFFFFFF: ";

FN PRINT_STRING(STR:2);
FN PRINT_HEX(N:2,SIZE:1);

FN MAIN() {
  VAR N:8=$FFFFFFFFFFFFFFFF;
  PRINT_STRING(STR);
  PRINT_HEX(@N,8);
  MON_CROUT();
} 

