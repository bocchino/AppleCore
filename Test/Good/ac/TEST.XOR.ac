# -------------------------------------
# TEST EXCLUSIVE OR
# -------------------------------------

CONST MON_CROUT  $FD8E;

DATA STR1 "SHOULD BE 8080FFFF: ";
DATA STR2 "SHOULD BE 7F7F7F7F: ";

FN PRINT_STRING(STR:2);
FN PRINT_HEX(N:2,SIZE:1);

FN MAIN() {
   VAR N:4 = $8080FFFF;
   PRINT_STRING(STR1);   
   PRINT_HEX(@N,4);
   MON_CROUT();
   PRINT_STRING(STR2);
   SET N = N XOR $FFFF8080;
   PRINT_HEX(@N,4);
   MON_CROUT();
}

INCLUDE "IO";
