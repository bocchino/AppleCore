# -------------------------------------
# TEST 4-BYTE UNSIGNED DIVISION
# -------------------------------------

CONST MON_CROUT  $FD8E;

DATA STR_A  "SHOULD BE 0010FFEF: ";
DATA STR_B  "SHOULD BE 00000011: ";
DATA STR_C  "SHOULD BE 0000FFFF: ";

FN PRINT_STRING(STR_PTR:2);
FN PRINT_NUM(N_PTR:2,SIZE:1);

FN MAIN() {
   VAR A:4 = $10FFEF;
   VAR B:4 = $11;
   VAR C:4 = A/B;
   # PRINT A
   PRINT_STRING(STR_A);   
   PRINT_NUM(@A,4);
   MON_CROUT();
   # PRINT B
   PRINT_STRING(STR_B);
   PRINT_NUM(@B,4);
   MON_CROUT();
   # PRINT C
   PRINT_STRING(STR_C);   
   PRINT_NUM(@C,4);
   MON_CROUT();
}

INCLUDE "IO";
