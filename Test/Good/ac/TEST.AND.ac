# -------------------------------------
# TEST LOGICAL AND OF 4-BYTE VALUES
# -------------------------------------

CONST MON_CROUT  $FD8E;

DATA STR1 "SHOULD BE 8181FFFF: ";
DATA STR2 "SHOULD BE 81818181: ";

FN PRINT_STRING(STR_PTR:2);
FN PRINT_NUM(N_PTR:2,SIZE:1);

FN MAIN() {
   VAR N:4 = $8181FFFF;
   PRINT_STRING(STR1);   
   PRINT_NUM(@N,4);
   MON_CROUT();
   PRINT_STRING(STR2);
   SET N = N AND $FFFF8181;
   PRINT_NUM(@N,4);
   MON_CROUT();
}

INCLUDE "IO";
