# -------------------------------------
# TEST 4-BYTE SIGNED MULTIPLICATION
# -------------------------------------

DATA STR_A  "SHOULD BE FFFFFFFF: ";
DATA STR_B  "SHOULD BE FFFFFFFE: ";
DATA STR_C  "SHOULD BE 00000002: ";

FN MAIN() {
   VAR A:4S = -1;
   VAR B:4S = -2;
   VAR C:4S = A*B;
   # PRINT A
   PRINT_STRING(STR_A);   
   PRINT_HEX(@A,4);
   MON_CROUT();
   # PRINT B
   PRINT_STRING(STR_B);
   PRINT_HEX(@B,4);
   MON_CROUT();
   # PRINT C
   PRINT_STRING(STR_C);   
   PRINT_HEX(@C,4);
   MON_CROUT();
}

INCLUDE "IO";
